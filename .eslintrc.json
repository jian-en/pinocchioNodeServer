{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "google",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [
    "json"
  ],
  "rules": {
    "max-len": ["error",
      {"ignoreComments": true},
      {"code": 100}
    ]
  }
}