{
  "name": "pinocchioserver",
  "version": "1.0.0",
  "description": "RESTful server",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon ./server.js",
    "test": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pinocchio61/pinocchioNodeServer.git"
  },
  "author": "Pinocchio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinocchio61/pinocchioNodeServer/issues"
  },
  "homepage": "https://github.com/pinocchio61/pinocchioNodeServer#readme",
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "aws-sdk": "^2.479.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-validator": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nodemailer": "^6.2.1",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,md}": [
      "./node_modules/.bin/eslint src --fix",
