{
  "name": "pinocchioserver",
  "version": "1.0.0",
  "description": "RESTful server",
  "main": "index.js",
  "scripts": {
    "dev-env": "NODE_ENV=dev ./node_modules/.bin/nodemon ./server.js",
    "test-env": "NODE_ENV=test ./node_modules/.bin/nodemon ./server.js",
    "create-tables": "node src/dynamodb/createTables.js",
    "load-initial-data": "node src/dynamodb/loadValidDomains.js",
    "load-test-data": "node src/tests/testData/loadData.js",
    "delete-tables": "node src/dynamodb/deleteTables.js",
    "test": "NODE_ENV=test source run_tests.sh",
    "awstest": "NODE_ENV=awstest node ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pinocchio61/pinocchioNodeServer.git"
  },
  "author": "Pinocchio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinocchio61/pinocchioNodeServer/issues"
  },
  "homepage": "https://github.com/pinocchio61/pinocchioNodeServer#readme",
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-markdown": "^1.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "async": "^2.6.3",
    "aws-sdk": "^2.496.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "ethereumjs-tx": "^2.1.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.0",
    "jquery-csv": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "moment": "^2.24.0",
    "nodemailer": "^6.2.1",
    "swagger-jsdoc": "^3.3.0",
    "swagger-ui-express": "^4.0.7",
    "web3": "^1.0.0-beta.55"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "./node_modules/.bin/eslint src --fix",
      "git add"
    ],
    "server.js": [
      "./node_modules/.bin/eslint server.js --fix",
      "git add"
    ],
    "README.md": [
      "./node_modules/.bin/eslint README.md --fix",
      "git add"
    ]
  }
}
